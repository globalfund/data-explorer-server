{
  "dataPath": "value",
  "year": "disbursementYear",
  "disbursed": "disbursed",
  "component": "componentName",
  "componentColors": {
    "HIV": "#70777E",
    "Tuberculosis": "#252C34",
    "Malaria": "#495057",
    "RSSH": "#ADB5BD",
    "Multicomponent": "#373D43"
  },
  "disbursementsTimeCycleAggregation": "$apply=groupby((DisbursementYear,ComponentName),aggregate(DisbursementAmount with sum as disbursed))"
}
