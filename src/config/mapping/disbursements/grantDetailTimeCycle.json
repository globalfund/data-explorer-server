{
  "dataPath": "value",
  "year": "disbursementYear",
  "disbursed": "disbursed",
  "component": "grantAgreementImplementationPeriod.grantAgreement.component.componentName",
  "componentColors": {
    "HIV": "#252C34",
    "Tuberculosis": "#595C70",
    "Malaria": "#B2B2B2",
    "TB/HIV": "#CDD4DF",
    "RSSH": "#A598F3",
    "Multicomponent": "#BFCFEE"
  },
  "disbursementsTimeCycleAggregation": "$apply=<filterString>groupby((DisbursementYear,grantAgreementImplementationPeriod/grantAgreement/component/componentName),aggregate(DisbursementAmount with sum as disbursed))"
}
