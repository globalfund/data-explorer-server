{
  "dataPath": "value",
  "component": "component.componentName",
  "locationCode": "geographicArea.geographicAreaCode_ISO3",
  "locationName": "geographicArea.geographicAreaName",
  "multicountry": "multiCountryName",
  "amount": "amount",
  "aggregation": "$apply=<filterString>groupby((component/componentName,geographicArea/geographicAreaCode_ISO3,geographicArea/geographicAreaName,multiCountryName),aggregate(allocationAmount with sum as amount))"
}
