{
  "dataPath": "value",
  "amount": "amount",
  "component": "component.componentName",
  "locationCode": "geographicArea.geographicAreaCode_ISO3",
  "geographicAreaMembers": "geographicArea.members",
  "multicountry": "multiCountryName",
  "multicountryComponent": "component.componentName",
  "multiCountryComposition": "multiCountryComposition",
  "multiCountryCompositionItem": "geographicArea.geographicAreaCode_ISO3",
  "aggregation": "$apply=<filterString>groupby((geographicArea/geographicAreaCode_ISO3,component/componentName),aggregate(allocationAmount with sum as amount))",
  "aggregationMulticountry": "$apply=<filterString>groupby((multiCountryName,component/componentName),aggregate(allocationAmount with sum as amount))"
}
