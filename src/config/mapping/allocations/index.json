{
  "dataPath": "value",
  "amount": "amount",
  "component": "component.componentName",
  "componentColors": {
    "HIV": "#595C70",
    "Tuberculosis": "#1B2127",
    "Malaria": "#727F95"
  },
  "allocationsAggregation": "$apply=<filterString>groupby((component/componentName),aggregate(allocationAmount with sum as amount))"
}
